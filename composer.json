{
  "name": "apsonex/laravel-document",
  "authors": [
    {
      "name": "Apsonex Inc.",
      "email": "apsonexinc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "intervention/image": "^2.7"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19",
    "nunomaduro/collision": "^6.1",
    "orchestra/testbench": "^7.1",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Apsonex\\LaravelDocument\\": "src",
      "Apsonex\\LaravelDocument\\Database\\Factories\\": "database/factories"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Apsonex\\LaravelDocument\\Tests\\": "tests"
    }
  },
  "scripts": {},
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Apsonex\\LaravelDocument\\DocumentServiceProvider"
      ],
      "aliases": {
        "Document": "Apsonex\\LaravelDocument\\Facades\\Document"
      }
    }
  },
  "repositories": [
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
